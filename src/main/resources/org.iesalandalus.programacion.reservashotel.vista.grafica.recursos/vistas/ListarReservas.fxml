<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="300.0" prefWidth="663.0" spacing="20.0" styleClass="vbox" stylesheets="@../estilos/estilos.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.iesalandalus.programacion.reservashotel.vista.grafica.controladores.ControladorListarReservas">
   <children>
      <Label style="-fx-font-size: 25;" text="Reservas" />
      <TableView fx:id="tvReservas" prefHeight="359.0" prefWidth="611.0">
        <columns>
          <TableColumn fx:id="tcProfesor" prefWidth="165.0" text="Profesor" />
          <TableColumn fx:id="tcAula" prefWidth="149.0" text="Aula" />
          <TableColumn fx:id="tcDia" prefWidth="108.0" text="Dia" />
          <TableColumn fx:id="tcHoraTramo" prefWidth="108.0" text="Hora o Tramo" />
          <TableColumn fx:id="tcPuntos" prefWidth="97.0" text="Puntos" />
        </columns>
      </TableView>
      <Button fx:id="btAceptar" mnemonicParsing="false" onAction="#aceptar" text="Aceptar" />
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
