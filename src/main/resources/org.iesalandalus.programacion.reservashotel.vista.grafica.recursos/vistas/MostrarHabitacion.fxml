<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="160.0" prefWidth="326.0" stylesheets="@../estilos/estilos.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.iesalandalus.programacion.reservasaulas.mvc.vista.iugventanas.controladoresvistas.ControladorMostrarAula">
   <children>
      <GridPane hgap="10.0" prefHeight="101.0" prefWidth="326.0" vgap="10.0">
         <children>
            <Label style="-fx-font-weight: bold;" text="Nombre:" GridPane.halignment="RIGHT" />
            <Label style="-fx-font-weight: bold;" text="Puestos:" GridPane.halignment="RIGHT" GridPane.rowIndex="1" />
            <Label fx:id="lbNombre" prefHeight="30.0" prefWidth="200.0" GridPane.columnIndex="1" />
            <Label fx:id="lbPuestos" prefHeight="30.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
         </children>
         <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="165.0" minWidth="10.0" prefWidth="97.0" />
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="217.0" minWidth="10.0" prefWidth="199.0" />
         </columnConstraints>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
         <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
         </rowConstraints>
      </GridPane>
      <HBox alignment="CENTER" prefHeight="51.0" prefWidth="326.0" spacing="20.0">
         <children>
            <Button fx:id="btBorrar" mnemonicParsing="false" onAction="#borrar" text="Borrar" />
            <Button fx:id="btAceptar" defaultButton="true" mnemonicParsing="false" onAction="#aceptar" text="Aceptar" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </HBox>
   </children>
</VBox>
